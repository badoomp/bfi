{{ attach_library('bfi/team-member-row-styling') }}

<div class="container team-member-row">
	<div class="row">
		<div class="col-sm-3 col-md-3 image" data-aos="fade-up">
			{{ content.field_image.0 }}
		</div>
		<div class="col-sm-9 col-md-9" data-aos="fade-up">
			<div class="name">
				{{ content.field_name.0 }}
			</div>
			<div class="description">
				{{ content.field_description.0 }}
			</div>
		</div>
	</div>
</div>